

<div>

    <h1> Speakers </h1>

<div class="container" style="background-image: url("../landing_PAge2.svg");  "> 

    <% for(var i=0; i<speakers.length; i++) { %>
        <div class="container">
           <div class="thumbnail">
                <img src="<%= speakers.img_url %>" height="250px" width="250px" />
            </div>
        <div class="desc">
            <div class="bold"><%= speakers.name %> </div>
            <div class="lead"> <%= speakers.desc %> </div>
        </div>
        </div>
    <% } %>
</div>
</div>